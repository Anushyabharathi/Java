YM1054
//print maximum alement in the array
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int max=arr[0];
        for(int i=1;i<n;i++)
        {
            if(arr[i]>max)
            {
                max=arr[i];
            }
        }
        System.out.println(max+" is the maximum element in the array");
    }
}


YM1055
// calculate the total number of apples in the fused matrix.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr1[]=new int[n];
        int arr2[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr1[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            arr2[i]=sc.nextInt();
        }
        int sum=0;
        for(int i=0;i<n;i++)
        {
            sum=arr1[i]+arr2[i];
            System.out.print(sum+" ");
        }
    }
}

YM1056
//to determine if Bob is more skilled or equally skilled compared to Alice in every round.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr1[]=new int[n];
        int arr2[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr1[i]=sc.nextInt();
        }
        for(int j=0;j<n;j++)
        {
            arr2[j]=sc.nextInt();
        }
        int sum=0;
        for(int i=0;i<n;i++)
        {
            if(arr1[i]>=arr2[i])
            {
                sum++;
            }
        }
        if(sum==n)
        {
            System.out.println("Compatibility Achieved!");
        }
        else
        {
            System.out.println("Incompatibility Detected!");
        }
    }
}


YM1057
//whether the elusive artifact is discovered among the magical treasures or not.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
       {
            arr[i]=sc.nextInt();
        }
        int m=sc.nextInt();
        int sum=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i]==m)
            {
                sum++;
            }
        }
        if(sum==1)
        {
           System.out.println("Elusive Artifact Found!");   
        }else
        {
            System.out.println("Elusive Artifact Not Found!");
        }
    }
}


YM1058
//Reveal a single integer, a testament to the count of distinct celestial notes within the cosmic array.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int count=0;
        for(int i=0;i<n-1;i++)
        {
            if(arr[i]==arr[i+1])
            {
                count++;
            }
        }
        int result=n-count;
        System.out.println(result);
    }
}


YM1059
//whether "Pappu successfully found the sum of even numbers in the array and 
//if he deserves the Diary Milk reward. or "Sorry Pappu, no Diary Milk this time. There are no even numbers in the array."
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int sum=0;
        int evencount=0,oddcount=0;
        for(int i=0;i<n;i++)
        {
            if(arr[i]%2==0)
            {
                evencount++;
            }
            else
            {
                oddcount++;
            }
        }
        if(oddcount==n)
        {
            System.out.println("Sorry Pappu, no Diary Milk this time. There are no even numbers in the array.");
        }
        else if(evencount<=n)
        {
            for(int i=0;i<n;i++)
            {
                if(arr[i]%2==0)
                {
                    sum+=arr[i];
                }
            }
             System.out.println("Pappu, the Diary Milk is yours! The sum of even numbers in the array is "+sum+".");
        }
    }
}



YM1060
//Output the sums of even and odd numbers, marking the successful completion of the "Number Quest Duel."
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        int evensum=0,oddsum=0;
        System.out.println("Victory in the Number Quest Duel!");
        for(int i=0;i<n;i++)
        {
            if(arr[i]%2==0)
            {
                evensum+=arr[i];
            }
            else
            {
                oddsum+=arr[i];
            }
        }
        if(evensum>0 || oddsum>0)
        {
            System.out.println("Sum of even numbers: "+evensum+".");
            System.out.println("Sum of odd numbers: "+oddsum+".");
        }
    }
}


YM1061
//Print "Match Found!" if the two families are the same. Print "No Match Found!" if the two families are different.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr1[]=new int[n];
        int arr2[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr1[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            arr2[i]=sc.nextInt();
        }
        int count=0;
        for(int i=0;i<n;i++)
        {
            if(arr1[i]==arr2[i])
            {
                count++;
            }
        }
        if(count==n)
        {
            System.out.println("Match Found!");
        }
        else
        {
            System.out.println("No Match Found!");
        }
    }
}


YM1062
//Output consists of a double value, representing the mean of the quantum array, printed up to 2 digits of precision.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        System.out.println("Quantum Mean Calculation Completed:");
        double sum=0;
        for(int i=0;i<n;i++)
        {
            sum+=arr[i];
        }
        double result=sum/n;
        System.out.println("The quantum mean of the array is "+result+"0");
        System.out.println("Congratulations, Quantum Explorer!");
    }
}


YM1063
//delete the specific position element in the arr
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        System.out.println("Array after deletion is");
        int pos=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            if(i!=pos-1)
            {
                System.out.println(arr[i]);
            }
        }
    }
}
