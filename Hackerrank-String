YM1070
//print the length of the string
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        System.out.println("Quantum revelation! The quantum length of "+a+" is "+a.length()+".");
    }
}

YM1071
//check two strings equal or not
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        String b=sc.nextLine();
        if(a.equals(b))
        {
            System.out.println("Celestial Confirmation! The strings align.");
        }
        else
        {
            System.out.println("Galactic Discrepancy! The strings do not align.");
        }
    }
}


YM1072
// finding the concatenation of the given two strings using string library functions.
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        String b=sc.nextLine();
        System.out.println("Friendship Rekindled! The concatenated string is "+a.concat(b)+".");
    }
}


YM1073
