import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        System.out.println("Word Ascension Achieved! Behold the majestic transformation: "+a.toUpperCase()+".");
    }
}


YM1074
//check whether the string is plindrome or not
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String b=sc.nextLine();
        StringBuffer a=new StringBuffer(b);
        StringBuffer c=a.reverse();
        String str=c.toString();
        if(str.equals(b))
        {
            System.out.println("Palindrome Revelation! The string is a palindrome.");
        }
        else
        {
            System.out.println("Palindromic Mystery Unsolved! The string is not a palindrome.");
        }
    }
}


YM1075
//togglecase
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        char c,toggle=0,cas=0;
        System.out.print("Case Cascade Magic Unleashed! The ");
        for(int i=0;i<a.length();i++)
        {
            c=a.charAt(i);
            toggle=(c>='A' && c<='Z')?(char)(c+32):(char)(c-32);
            System.out.print(toggle);
        }
        System.out.println(" transformation is complete.");
    }
}


YM1076
//print the number of vowels present in the string
import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String a=sc.nextLine();
        int count=0;
        char c;
        for(int i=0;i<a.length();i++)
        {
            c=Character.toLowerCase(a.charAt(i));
            if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u')
            {
                 count++;
            }
        }
        System.out.println("Vowel Voyage Completed! Number of vowels: "+count);
    }
}
